meta {
  name: Cleanup Old Articles
  type: http
  seq: 2
}

delete {{baseUrl}}/api/system/cleanup {
  headers {
    X-API-Key: {{apiKey}}
  }
  query {
    months: 6
  }
}

tests {
  test("status should be 200", () => {
    expect(res.getStatus()).toBe(200);
  });
  test("should return cleanup results", () => {
    const response = res.getBody();
    expect(response.data.deletedCount).toBeDefined();
    expect(response.data.monthsOld).toBe(6);
  });
}