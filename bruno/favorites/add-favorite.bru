meta {
  name: Add Feed to Favorites
  type: http
  seq: 1
}

post {{baseUrl}}/api/favorites {
  headers {
    Content-Type: application/json
    X-API-Key: {{apiKey}}
    Authorization: Bearer {{accessToken}}
  }
  body {
    {
      "feed_id": "{{feedId}}"
    }
  }
}

tests {
  test("status should be 201", () => {
    expect(res.getStatus()).toBe(201);
  });
  const response = res.getBody();
  bru.setVar("favoriteId", response.data.id);
}