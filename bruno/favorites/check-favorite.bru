meta {
  name: Check Favorite Status
  type: http
  seq: 4
}

get {{baseUrl}}/api/favorites/check/{{feedId}} {
  headers {
    X-API-Key: {{apiKey}}
    Authorization: Bearer {{accessToken}}
  }
}

tests {
  test("status should be 200", () => {
    expect(res.getStatus()).toBe(200);
  });
  test("should return favorite status", () => {
    const response = res.getBody();
    expect(response.isFavorite).toBeDefined();
  });
}